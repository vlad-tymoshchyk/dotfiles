[battery]
interval=60
command=echo "|Battery: $(upower -i $(upower -e | grep 'BAT')| grep percentage | awk 'match($0, /[0-9]+%/) { print substr( $0, RSTART, RLENGTH )}') ($(upower -i $(upower -e | grep 'BAT')| grep state | awk '{ print $2 }'))|"

[time]
command=echo "[$(date +%x)] $(date +%H:%M)"
interval=60

[volume]
interval=once
signal=10

[help]
interval=once
